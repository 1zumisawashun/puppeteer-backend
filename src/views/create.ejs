<!DOCTYPE html>
<html lang="ja">
  <%- include("./partials/head.ejs") %>
  <body>
    <%- include("./partials/nav.ejs") %>

    <div class="create-blog content">
      <form action="/lists" method="POST" enctype="multipart/form-data">
        <label for="productName">Product Name:</label>
        <input type="text" id="productName" name="productName" required />
        <label for="shopNamePath">Shop Name Path:</label>
        <input type="text" id="shopNamePath" name="shopNamePath" required />
        <label for="productPricePath">Product Price Path:</label>
        <input
          type="text"
          id="productPricePath"
          name="productPricePath"
          required
        />
        <label for="productPricePath">Url:</label>
        <input type="text" id="url" name="url" required />
        <label for="remark">Remark:</label>
        <textarea id="remark" name="remark" required></textarea>
        <label for="file">File:</label>
        <input type="file" id="file" name="file" required />
        <button>Submit</button>
      </form>
    </div>
    <%- include("./partials/footer.ejs") %>
  </body>
</html>
